<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="${EPSG3857}" background-color="#ffffff">

<Style name="hillshade">
  <Rule>
    <RasterSymbolizer scaling="bilinear" />
  </Rule>
</Style>

<Layer name="hillshade" srs="${EPSG3857}">
    <StyleName>hillshade</StyleName>
    <Datasource>
      <Parameter name="file">../data/hillshade.vrt</Parameter>
      <Parameter name="type">gdal</Parameter>
    </Datasource>
</Layer>

</Map>
